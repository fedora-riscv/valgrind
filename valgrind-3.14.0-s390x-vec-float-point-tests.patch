+typedef enum {
+   V128_NO_PRINTING = 0,
+   V128_V_RES_AS_INT  = 1 << 0,
+   V128_V_ARG1_AS_INT = 1 << 1,
+   V128_V_ARG2_AS_INT = 1 << 2,
